<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_nineth"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.alisha.justguess2.nineth">

    <Button
        android:text="_"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/m91"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true"
        android:background="@mipmap/p1"
        />

    <Button
        android:text="O"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/m92"
        android:layout_marginStart="15dp"
        android:layout_alignParentTop="true"
        android:layout_toEndOf="@+id/m91"
        android:background="@mipmap/pu1"
        />

    <Button
        android:text="A"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/m95"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:background="@mipmap/p1"
        />

    <Button
        android:text="_"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/m94"
        android:layout_marginStart="20dp"
        android:layout_toEndOf="@+id/m93"
        android:background="@mipmap/pu1"
        android:layout_alignParentTop="true" />

    <Button
        android:text="_"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:id="@+id/m93"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:background="@mipmap/p1"
        />

    <TextView
        android:layout_width="200dp"
        android:layout_height="45dp"
        android:layout_alignBottom="@+id/m9h"
        android:id="@+id/m9turns"
        android:background="@mipmap/p1"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/m94" />

    <EditText
        android:layout_width="150dp"
        android:layout_height="45dp"
        android:inputType="textPersonName"
        android:hint="enter your letter"
        android:ems="10"
        android:id="@+id/m9ed"
        android:layout_centerVertical="true"
        android:layout_toStartOf="@+id/m25"
        android:background="@mipmap/p1"
        />

    <Button
        android:text="ENTER"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:id="@+id/m9enter"
        android:layout_centerVertical="true"
        android:layout_alignEnd="@+id/m9h"
        android:background="@mipmap/pu1"
        />

    <Button
        android:text="HINT"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:id="@+id/m9h"
        android:layout_marginEnd="14dp"
        android:background="@mipmap/pu1"
        android:layout_below="@+id/m9enter"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="72dp" />

    <Button
        android:text="NEXT"
        android:layout_width="100dp"
        android:layout_height="45dp"
        android:id="@+id/m9next"
        android:background="@mipmap/pu1"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/m92" />


</RelativeLayout>
